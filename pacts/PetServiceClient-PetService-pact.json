{
  "consumer": {
    "name": "PetServiceClient"
  },
  "provider": {
    "name": "PetService"
  },
  "interactions": [
    {
      "providerStates": [
        {
          "name": "Pet Order is Ready",
          "params": {}
        }
      ],
      "description": "a request for order",
      "request": {
        "method": "get",
        "path": "/store/order/3"
      },
      "response": {
        "status": 200,
        "body": {
          "id": 0,
          "petId": 0,
          "shipDate": "1944-02-07T13:54:19.92Z",
          "quantity": 0,
          "status": "placed",
          "complete": false
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.shipDate": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "\\d+-\\d+-\\d+T\\d+:\\d+:\\d+.\\d+Z"
                }
              ]
            },
            "$.status": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "\\w+"
                }
              ]
            }
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    }
  }
}