{
  "consumer": {
    "name": "UserServiceClient"
  },
  "provider": {
    "name": "UserService"
  },
  "interactions": [
    {
      "providerStates": [
        {
          "name": "UserA exists and is not an administrator",
          "params": {}
        }
      ],
      "description": "a request for UserA",
      "request": {
        "method": "get",
        "path": "/users/UserA"
      },
      "response": {
        "status": 200,
        "body": {
          "name": "UserA",
          "id": "00000000-0000-4000-a000-000000000000",
          "created_on": "2016-12-15-20:16:01",
          "admin": false
        },
        "matchingRules": {
          "body": {
            "$": {
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.id": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "^[a-f0-9]{8}-?[a-f0-9]{4}-?4[a-f0-9]{3}-?[89ab][a-f0-9]{3}-?[a-f0-9]{12}\\Z"
                }
              ]
            },
            "$.created_on": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "\\d+-\\d+-\\d+-\\d+:\\d+:\\d+"
                }
              ]
            }
          }
        }
      }
    },
    {
      "providerStates": [
        {
          "name": "UserB does not exist",
          "params": {}
        }
      ],
      "description": "a request for UserB",
      "request": {
        "method": "get",
        "path": "/users/UserB"
      },
      "response": {
        "status": 404
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    }
  }
}